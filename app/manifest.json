{
  "name": "Tab Audio Equalizer",
  "description": "Apply equalizer and compressor effects to audio from any Chrome tab",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabCapture",
    "activeTab"
  ],
  "action": {
    "default_popup": "page.tsx",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}